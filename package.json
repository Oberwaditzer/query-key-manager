{
  "name": "@ocodio/query-key-manager",
  "version": "0.0.4",
  "private": false,
  "description": "Utility helpers for organizing TanStack Query keys.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --dts --format esm,cjs --clean",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "check": "pnpm lint && pnpm test && pnpm build",
    "docs": "typedoc",
    "release": "changeset version && pnpm i --lockfile-only && git add -A && git commit -m \"chore: release versions\" && pnpm publish -r",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "@changesets/cli": "^2.27.9",
    "@eslint/js": "^9.37.0",
    "@tanstack/react-query": "^5.90.2"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Oberwaditzer/query-key-manager"
  }
}
